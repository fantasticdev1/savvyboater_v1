{"components/products/description":"<div class=\"productView-description\" {{#if settings.data_tag_enabled}} data-event-type=\"product\" {{/if}}>\r\n    {{{product.description}}}\r\n    {{{snippet 'product_description'}}}\r\n</div>\r\n<div class=\"Block Moveable Panel\" id=\"ProductReviews\">\r\n    <div id=\"review_header\">\r\n    </div>\r\n    <div id=\"product_page\">\r\n    </div>\r\n    <div id=\"review_image\">\r\n        <a href=\"http://www.shopperapproved.com/reviews/savvyboater.com/\" onclick=\"var nonwin=navigator.appName!='Microsoft Internet Explorer'?'yes':'no'; var certheight=screen.availHeight-90; window.open(this.href,'shopperapproved','location='+nonwin+',scrollbars=yes,width=620,height='+certheight+',menubar=no,toolbar=no'); return false;\" target=\"_blank\" rel=\"nofollow\"></a>\r\n    </div>\r\n    <script type=\"text/javascript\">\r\n        $(window).load(function() {\r\n            if(!$('div.merchantheader').length){\r\n                $('div#review_header').prepend('<div class=\"merchantheader\">The reviews shown may include reviews for similiar products, and are not necessarily all for this specific product.</div>');\r\n            }\r\n        });\r\n    </script>\r\n</div>\n"}

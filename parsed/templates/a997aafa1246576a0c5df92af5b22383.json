{"components/products/options/set-select":"<div class=\"form-field\" data-product-attribute=\"set-select\" {{#if this.display_name \"===\" \"Fabric Type\"}} id=\"fabric-type\" {{/if}}>\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"attribute_select_{{id}}\">\r\n        {{ this.display_name }}:\r\n\r\n        {{#if required}}\r\n            <small>{{lang 'common.required'}}</small>\r\n        {{/if}}\r\n    </label>\r\n\r\n    <select class=\"validation form-select form-select--small {{#if this.display_name \"===\" \"Fabric Type\"}} customDropDown {{/if}}\" name=\"attribute[{{this.id}}]\" id=\"attribute_select_{{id}}\" {{#if required}}required{{/if}}>\r\n        <option value=\"\">{{#if this.display_name \"===\" \"Fabric Type\"}}Select Fabric{{else}}{{lang 'products.choose_options'}}{{/if}}</option>\r\n        {{#each this.values}}\r\n            <option data-product-attribute-value=\"{{id}}\" data-product-attribute-label=\"{{label}}\" value=\"{{id}}\" {{#if selected}}selected data-default{{/if}}>{{label}}</option>\r\n        {{/each}}\r\n    </select>\r\n</div>\r\n"}

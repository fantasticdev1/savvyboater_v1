\n        <!--<li><a href=\"/privacy-policy/\">Privacy Policy</a>-->\r\n        <li><a href=\"/brands/\">Shop Our Brands</a>\r\n        </li>\r\n        <li><a href=\"//savvyboater.com/blog/\">Follow Our Blog</a>\r\n        </li>\r\n      </ul>\r\n    </div>\r\n    <div class=\"FootCol\">\r\n      <div class=\"adrs\">\r\n          <img src=\"https://res.cloudinary.com/www-savvyboater-com/image/upload/q_auto:good/v1558470847/homepage/logo-footer.jpg\">\r\n        <h2>866-560-1330</h2>\r\n        <p>Monday - Friday</p>\r\n        <p>7:00 am - 4:00 pm PST</p>\r\n      </div>\r\n    </div>\r\n  </div>\r\n  </div>\r\n\r\n  <div class=\"social_sec\">\r\n    <div class=\"innerheader\">\r\n      <div class=\"FootCol\">\r\n      <div class=\"social_div\">\r\n        <ul class=\"SocialIcons\">\r\n          <li><a href=\"https://www.facebook.com/savvyboater\" target=\"_blank\"><img src=\"https://res.cloudinary.com/www-savvyboater-com/image/upload/q_auto:good/v1558470759/homepage/icon-facebook.jpg\">\r\n          </a></li>\r\n          <li><a href=\"https://www.youtube.com/user/SavvyBoater\" target=\"_blank\"><img src=\"https://res.cloudinary.com/www-savvyboater-com/image/upload/q_auto:good/v1558470781/homepage/icon-youtube.jpg\">\r\n          </a></li>\r\n          <li><a href=\"http://www.shopperapproved.com/reviews/savvyboater.com/\" target=\"_blank\"><img src=\"https://res.cloudinary.com/www-savvyboater-com/image/upload/q_auto:good/v1558470802/homepage/icon-shopper.png\">\r\n          </a></li>\r\n          <li><a href=\"https://smarticon.geotrust.com/smarticonprofile?Referer=https://www.savvyboater.com\" target=\"_blank\"><img src=\"https://res.cloudinary.com/www-savvyboater-com/image/upload/q_auto:good/v1558470824/homepage/icon-geotrust.jpg\">\r\n          </a></li>\r\n\r\n        </ul>\r\n\r\n      </div>\r\n\r\n    </div>\r\n    </div>\r\n  </div>\r\n    <div class=\"FooterCopyright\">\r\n  <p>\r\n    Copyright © {{moment format=\"YYYY\"}} SavvyBoater - All Rights Reserved. 7291 SW Tech Center Dr. Tigard, OR 97223</p>\r\n</div>\r\n","components/common/store-logo":"<a href=\"{{urls.home}}\">\r\n    {{#if settings.store_logo.image}}\r\n        {{#if theme_settings.logo_size '===' 'original'}}\r\n            <img class=\"header-logo-image-unknown-size\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"Discount Boat Supplies and Accessories\" title=\"{{settings.store_logo.title}}\">\r\n        {{else}}\r\n            <div class=\"header-logo-image-container\">\r\n                <img class=\"header-logo-image\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"Discount Boat Supplies and Accessories\" title=\"{{settings.store_logo.title}}\">\r\n            </div>\r\n        {{/if}}\r\n    {{else}}\r\n        <span class=\"header-logo-text\">{{settings.store_logo.title}}</span>\r\n    {{/if}}\r\n</a>\r\n","components/common/navigation-menu":"<nav class=\"navPages\">\r\n    <div class=\"navPages-quickSearch\">\r\n        {{> components/common/quick-search}}\r\n    </div>\r\n    <ul class=\"navPages-list{{#if theme_settings.navigation_design '!==' 'simple'}} navPages-list-depth-max{{/if}}\">\r\n        {{#each categories}}\r\n            <li class=\"navPages-item\">\r\n            {{#if ../theme_settings.navigation_design '===' 'simple'}}\r\n                {{> components/common/navigation-list}}\r\n            {{/if}}\r\n            {{#if ../theme_settings.navigation_design '===' 'alternate'}}\r\n                {{> components/common/navigation-list-alternate}}\r\n            {{/if}}\r\n            </li>\r\n        {{/each}}\r\n        {{#unless theme_settings.hide_content_navigation}}\r\n            {{#each pages}}\r\n                <li class=\"navPages-item navPages-item-page\">\r\n                    {{#if children}}\r\n                    <a class=\"navPages-action navPages-action-depth-max has-subMenu is-root{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\" data-collapsible=\"navPages-{{dashcase name}}\">\r\n                        {{name}} <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i>\r\n                    </a>\r\n                    <div class=\"navPage-subMenu navPage-subMenu-horizontal\" id=\"navPages-{{dashcase name}}\" aria-hidden=\"true\" tabindex=\"-1\">\r\n                        <ul class=\"navPage-subMenu-list\">\r\n                            {{#each children}}\r\n                                <li class=\"navPage-subMenu-item-child\">\r\n                                    {{#if children}}\r\n                                        <a class=\"navPage-subMenu-action navPages-action navPages-action-depth-max has-subMenu{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\" data-collapsible=\"navPages-{{id}}\">\r\n                                            {{name}} <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i>\r\n                                        </a>\r\n                                        {{> components/common/navigation-dropdown}}\r\n                                    {{else}}\r\n                                        <a class=\"navPage-subMenu-action navPages-action{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\">{{name}}</a>\r\n                                    {{/if}}\r\n                                </li>\r\n                            {{/each}}\r\n                        </ul>\r\n                    </div>\r\n                    {{else}}\r\n                    <a class=\"navPages-action\" href=\"{{url}}\">{{name}}</a>\r\n                    {{/if}}\r\n                </li>\r\n             {{/each}}        \r\n        {{/unless}}\r\n    </ul>\r\n    <ul class=\"navPages-list navPages-list--user\">\r\n        {{#if currency_selector.currencies.length '>' 1}}\r\n            <li class=\"navPages-item\">\r\n                <a class=\"navPages-action has-subMenu\" href=\"#\"
{"components/products/options/set-radio":"<div class=\"productAttributeRow productAttributeConfigurablePickListProduct borderbottom form-field\" data-product-attribute=\"set-radio\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall productAttributeLabel\">\r\n        {{ this.display_name }}:\r\n\r\n        {{#if required}}\r\n            <small>{{lang 'common.required'}}</small>\r\n        {{/if}}\r\n    </label>\r\n    <div class=\"productAttributeValue\">\r\n    {{#each this.values}}\r\n        <input\r\n            class=\"form-checkbox customCheckBox\"\r\n            type=\"checkbox\"\r\n            id=\"attribute_radio_{{../id}}_{{id}}\"\r\n            name=\"attribute[{{../id}}]\"\r\n            value=\"{{id}}\"\r\n            {{#if selected}}\r\n                checked\r\n                data-default\r\n            {{/if}}\r\n            {{#if ../required}}required{{/if}}>\r\n        <label data-product-attribute-value=\"{{id}}\" class=\"form-label\" for=\"attribute_radio_{{../id}}_{{id}}\"><span>{{this.label}}</span></label>\r\n    {{/each}}\r\n    </div>\r\n</div>\r\n"}
